<div class="fixed_header">	
	<div class="row-content">
		<div class="column six logo">
			<% if @full_name != nil then %>
				<%= @full_name.split(" ").first %><span class="change_color"><%= @full_name.split(" ").last %></span><span class="on_gittofolio">on Gittofolio</span>
			<% else %>
				<%= params[:user_name] %>
			<% end %>
		</div>
		<div class="column six right reset">
			<ul class="nav reset">
				<li><%= link_to "Repository List", "/#{params[:user_name]}" %></li>
				<li><a href="#">About</a></li>
				<li><a href="#">Contact</a></li>
			</ul>
		</div>
	</div>
</div>
<div class="clear"></div>