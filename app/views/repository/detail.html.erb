<div class="small-12 columns">
	
	<ul class="inline-list right repo_date">
		<li>Created: <%= @repo.first.start_date %></li>
		<li>Updated: <%= @repo.first.update_date %></li>
	</ul>
	<div class="border_bottom"><h6><i class="fa fa-github"></i><%= params[:repo_name] %></h6></div>
	<% if @homepage != "not_available" %>
		<img src="http://s.wordpress.com/mshots/v1/<%= @homepage %>?w=1000">
	<% end %>
	<div class="row">
		<div class="large-4 columns">

		</div>
	</div>
</div>